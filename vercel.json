{
    "rewrites": [
      {
        "source": "/(.*)",
        "destination": "/"
      }
    ],
    "builds": [
      {
        "src": "package.json",
        "use": "@vercel/next",
        "config": {
          "buildCommand": "git lfs install && git lfs pull && next build"
        }
      }
    ]
  }
  